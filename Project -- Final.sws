                <textarea class="cell_input" rows="44" cols="80" spellcheck="false" id="cell_input_68" style="height: 729px;">def triple_intersections(tope):
    """Computes the triple intersection tensor for a given polytope"""
    verts = sorted(list(tope.vertices_pc()))
    k = len(verts)
    n = 4
    A = chow_group(tope)
    
    vol = 0
    stope = None
    
    combs = Combinations(k, 4)
    
    for comb in combs:
        [i, j, l, m] = comb
        stope = vind_to_tope(tope, [i, j, l, m])
        vol = volume(stope)
        if vol != 0:
            break           


    n_vol = volume(stope)
    [n1, n2, n3, n4] = [i, j, l, m]
    n_prod = A.gens()[n1+1]*A.gens()[n2+1]*A.gens()[n3+1]*A.gens()[n4+1]
    print n_prod, "======", n_vol
    
    X = sum(A.gens()[1:k+1])    # the Calabi-Yau
    intersections = np.empty(shape=(k-n,k-n,k-n))
    
    jcombs = []
    for i in range(k-n):
        for j in range(k-n):
            for k in range(k-n):
                jcombs.append([i, j, k])
    
    for comb in jcombs:
        #print comb
        [i, j, l] = comb
        prod = A.gens()[k+1+i]*A.gens()[k+1+j]*A.gens()[k+1+l]*X
        #print prod
        intr = prod / n_prod
        #print intr
        intr /= n_vol
        #print intr
        intersections[i, j, l] = int(round(intr, 0))
    
    return intersections</textarea>
